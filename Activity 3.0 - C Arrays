#include<stdio.h>

int main()
{
    int arr[10], size, i, j = 1, sum = 0;

    printf("Enter the size of the Array: ");
    scanf("%d",&size);

    for(i = 0; i < size; i++)
        {
          printf("Enter Element %d: ", j++);
          scanf("%d",&arr[i]);
        }

          for(i = 0; i < size; i++)
          sum = sum + arr[i];

    printf("Sum of all array elements = %d",sum);

    return 0;
}
